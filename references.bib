@article{palazon_autumn-winter_2008,
	title = {Autumn-winter diet of three carnivores, {European} mink ({Mustela} lutreola), {Eurasian} otter ({Lutra} lutra) and small-spotted genet ({Genetta} genetta), in northern {Spain}},
