	number = {2},
	urldate = {2024-01-17},
	journal = {Animal Biodiversity and Conservation},
	author = {Palazón, S. and Ruiz-Olmo, J. and Gosálbez, J.},
	month = dec,
	year = {2008},
	keywords = {Mustela lutreola, European mink, diet, Spain, Eurasian otter, Lutra lutra, Small-spotted genet, Genetta genetta, non lu},
	pages = {37--43},
	file = {Direcció General del Medi Natural, Barcelona, Spain et al. - 2008 - Autumn-winter diet of three carnivores, European m.pdf:C\:\\Users\\Rémi\\Zotero\\storage\\SKAZQJP9\\Direcció General del Medi Natural, Barcelona, Spain et al. - 2008 - Autumn-winter diet of three carnivores, European m.pdf:application/pdf},
