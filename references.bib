	keywords = {Mustela lutreola, European mink, diet, Spain, non lu},
	pages = {159--165},
	file = {Palazón et al. - 2004 - Diet of European mink ( Mustela lutreola ) .pdf:C\:\\Users\\Rémi\\Zotero\\storage\\66VPITBI\\Palazón et al. - 2004 - Diet of European mink ( Mustela lutreola ) .pdf:application/pdf},
