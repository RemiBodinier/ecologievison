
@article{maran_diet_1998,
	title = {Diet of two species of mink in {Estonia}: displacement of \textit{{Mustela} lutreola} by \textit{{M}. vison}},
	volume = {245},
	issn = {0952-8369, 1469-7998},
	shorttitle = {Diet of two species of mink in {Estonia}},
	url = {https://zslpublications.onlinelibrary.wiley.com/doi/10.1111/j.1469-7998.1998.tb00093.x},
	doi = {10.1111/j.1469-7998.1998.tb00093.x},
	abstract = {During a period in the 1980s when both European mink Mustela lutreola and American mink M. vison were present in Estonia, their food was signi®cantly different. European mink ate a greater proportion of ®sh and crustaceans, whereas American mink took relatively more mammals and frogs. This was probably related to a difference in habitat selection. After the disappearance of the European mink, the diet of the American mink in our main study area was similar to that of the European in the same area previously. Two alternative hypotheses are presented for the mechanisms which led to the replacement of European mink by the American species: (i) the two species have a different niche, and the American mink could replace the European mink after the latter had disappeared for unrelated reasons, or (ii) the American mink aggressively ousted the European mink, a process starting in the American mink's preferred habitat (slow ¯owing rivers). At present there are insuf®cient data to reject either of these scenarios.},
	language = {en},
	number = {2},
	urldate = {2024-01-17},
	journal = {Journal of Zoology},
	author = {Maran, T. and Kruuk, H. and Macdonald, D. W. and Polma, M.},
	month = jun,
	year = {1998},
	keywords = {competition, diet, mink, habitat, extinction, non lu},
	pages = {218--222},
	file = {Maran et al. - 1998 - Diet of two species of mink in Estonia displaceme.pdf:C\:\\Users\\Rémi\\Zotero\\storage\\MQQSD9UV\\Maran et al. - 1998 - Diet of two species of mink in Estonia displaceme.pdf:application/pdf},
}

@article{palazon_diet_2004,
	title = {Diet of {European} mink ( \textit{{Mustela} lutreola} ) in {Northern} {Spain}},
